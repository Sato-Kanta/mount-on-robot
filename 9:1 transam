	morphologyEx(mask, mask2, MORPH_OPEN, kernel);
	for (int i = 0; i < 4; i++) {
		morphologyEx(mask2, mask2, MORPH_OPEN, kernel);
		medianBlur(mask2, mask2, 5);
	}
	threshold(mask2, bgf1, 120, 255, THRESH_BINARY);
